{
  "name": "drizzle-seeder",
  "version": "0.2.0",
  "keywords": [
    "drizzle-orm",
    "prisma",
    "seeding"
  ],
  "homepage": "https://github.com/danolekh/drizzle-seeder#readme",
  "bugs": {
    "url": "https://github.com/danolekh/drizzle-seeder/issues"
  },
  "license": "MIT",
  "author": "Daniil Olekh <danyaolekhq@gmail.com> (https://github.com/danolekh)",
  "repository": {
    "type": "git",
    "url": "https://github.com/danolekh/drizzle-seeder"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./generators": {
      "types": "./dist/generators/index.d.mts",
      "import": "./dist/generators/index.mjs"
    },
    "./pg-core": {
      "types": "./dist/pg-core/index.d.mts",
      "import": "./dist/pg-core/index.mjs"
    },
    "./sqlite-core": {
      "types": "./dist/sqlite-core/index.d.mts",
      "import": "./dist/sqlite-core/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "check": "tsc --noEmit",
    "publish": "npm publish",
    "lint": "oxlint .",
    "fmt": "oxfmt .",
    "fmt:check": "oxfmt --check ."
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "devalue": "^5.6.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.3.6",
    "drizzle-orm": "1.0.0-beta.10-4a43a22",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "tsdown": "^0.19.0",
    "tsx": "^4.21.0",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "@faker-js/faker": "^10.2.0",
    "drizzle-orm": "1.0.0-beta.10-4a43a22",
    "typescript": "^5"
  }
}
